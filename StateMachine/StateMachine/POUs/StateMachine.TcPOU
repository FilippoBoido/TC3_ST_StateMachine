    <Method Name="Boot" Id="{752de0f2-d7a8-4119-aff0-0199bc482dc1}" FolderPath="Orders\">
      <Declaration><![CDATA[METHOD PROTECTED Boot : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF bFirstCycle
THEN
	ChangeState(eInit);
	bFirstCycle := FALSE;	
	Boot := TRUE;
END_IF]]></ST>
      </Implementation>
    </Method>
