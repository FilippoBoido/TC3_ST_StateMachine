    <Method Name="Done" Id="{0603ae73-2603-4bdb-ad58-96bbbc3ca43a}" FolderPath="Orders\">
      <Declaration><![CDATA[METHOD PROTECTED Done : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF eStateMachine = eBusy
THEN
